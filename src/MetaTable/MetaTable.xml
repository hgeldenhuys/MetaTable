<?xml version="1.0" encoding="utf-8" ?>
<widget id="MetaTable.widget.MetaTable" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>MetaTable</name>
    <description>The description of this widget.</description>

    <icon>
    
    </icon>

    <properties>
        <property key="tableName" type="attribute" isPath="optional" pathType="reference" allowNonPersistableEntities="true" isDefault="true" required="true">
            <caption>Table Name</caption>
            <category>Data</category>
            <description>Table name attribute</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="ignoreTables" type="string" defaultValue="System.*;">
            <caption>Ignored Tables</caption>
            <category>Filter</category>
            <description>Ignore these tables, use comma-separators. Example: System.User</description>
        </property>
        <property key="moduleLabel" type="translatableString" required="true">
            <caption>Module label</caption>
            <category>Display</category>
            <description>The label for the module select</description>
        </property>
        <property key="entityLabel" type="translatableString" required="true">
            <caption>Entity label</caption>
            <category>Display</category>
            <description>The label for the entity select</description>
        </property>
        <property key="onchangeMicroflow" type="microflow" required="false">
            <caption>On Change</caption>
            <category>Events</category>
            <description>On change microflow</description>
            <returnType type="Void"/>
        </property>

    </properties>
</widget>

